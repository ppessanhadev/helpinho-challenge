generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid()) @db.Uuid
  email    String   @db.Text
  password String   @db.Text
  name     String   @db.Text
  cpf      String   @db.Text
  birthday DateTime @db.Timestamptz()

  @@map("users")
}
